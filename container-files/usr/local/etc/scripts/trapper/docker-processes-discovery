#!/bin/sh

ZABBIX_SENDER=/usr/bin/zabbix_sender
ZABBIX_CONF=/usr/local/etc/zabbix/zabbix_agentd.conf

zsend () {
   $ZABBIX_SENDER -c $ZABBIX_CONF -k "$1" -o "$2" > /dev/null 2>&1
}

KEY="docker.discovery.processes"
VALUE=$(/usr/local/etc/scripts/discovery/docker-processes)
if [ "$?" -ne "0" ]; then
   exit 1
fi

zsend "$KEY" "$VALUE"
